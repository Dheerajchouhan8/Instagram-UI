<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
           <KeyAttribute
               motion:motionTarget="@+id/tabLayout"
               motion:framePosition="-100"
               android:translationY="30dp" />
           <KeyAttribute
               motion:motionTarget="@+id/imageView"
               motion:framePosition="0"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/floatingActionButton"
               motion:framePosition="0"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/textView4"
               motion:framePosition="0"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/textView"
               motion:framePosition="0"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/textView2"
               motion:framePosition="0"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/textView3"
               motion:framePosition="0"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/textView3"
               motion:framePosition="0"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/textView5"
               motion:framePosition="0"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/textView6"
               motion:framePosition="0"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/textView8"
               motion:framePosition="0"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/textView9"
               motion:framePosition="0"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/button"
               motion:framePosition="0"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/button2"
               motion:framePosition="0"
               android:alpha="0.5" />
           <KeyAttribute
               motion:motionTarget="@+id/imageButton"
               motion:framePosition="0"
               android:alpha="0.5" />
       </KeyFrameSet>
        <OnSwipe />
        <OnSwipe motion:touchAnchorId="@+id/textView7" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/tabLayout"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/button"
            motion:layout_constraintHorizontal_bias="0.272"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/imageView"
            android:layout_width="100dp"
            android:layout_height="100dp"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="50dp" />
        <Constraint
            android:id="@+id/floatingActionButton"
            motion:layout_constraintEnd_toEndOf="@+id/imageView"
            android:layout_width="50dp"
            android:layout_height="50dp"
            motion:layout_constraintBottom_toBottomOf="@+id/imageView" />
        <Constraint
            android:id="@+id/textView"
            motion:layout_constraintEnd_toEndOf="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView4"
            motion:layout_constraintStart_toStartOf="@+id/textView4" />
        <Constraint
            android:id="@+id/textView2"
            motion:layout_constraintEnd_toEndOf="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView5"
            motion:layout_constraintStart_toStartOf="@+id/textView5" />
        <Constraint
            android:id="@+id/textView3"
            motion:layout_constraintEnd_toEndOf="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView6"
            motion:layout_constraintStart_toStartOf="@+id/textView6" />
        <Constraint
            android:id="@+id/textView4"
            motion:layout_constraintStart_toEndOf="@+id/imageView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="50dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="80dp" />
        <Constraint
            android:id="@+id/textView5"
            motion:layout_constraintEnd_toStartOf="@+id/textView6"
            motion:layout_constraintStart_toEndOf="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="80dp" />
        <Constraint
            android:id="@+id/textView6"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="50dp"
            motion:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="80dp" />
        <Constraint
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintStart_toStartOf="parent" />
        <Constraint
            android:id="@+id/floatingActionButton2"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginEnd="50dp"
            motion:layout_constraintTop_toTopOf="parent" />
        <Constraint
            android:id="@+id/textView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/imageView"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp" />
        <Constraint
            android:id="@+id/textView9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            motion:layout_constraintTop_toBottomOf="@+id/textView8"
            motion:layout_constraintStart_toStartOf="@+id/textView8"
            android:layout_marginTop="4dp" />
        <Constraint
            android:id="@+id/button"
            android:layout_width="160dp"
            android:layout_height="50dp"
            motion:layout_constraintTop_toBottomOf="@+id/textView9"
            android:layout_marginStart="4dp"
            motion:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="20dp" />
        <Constraint
            android:id="@+id/imageButton"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_height="50dp"
            motion:layout_constraintTop_toTopOf="@+id/button2" />
        <Constraint
            android:id="@+id/button2"
            motion:layout_constraintEnd_toStartOf="@+id/imageButton"
            motion:layout_constraintStart_toEndOf="@+id/button"
            android:layout_width="160dp"
            android:layout_height="50dp"
            motion:layout_constraintTop_toTopOf="@+id/button" />
        <Constraint
            android:id="@+id/viewPager"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="0dp"
            android:layout_height="0dp"
            motion:layout_constraintBottom_toBottomOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/tabLayout"
            motion:layout_constraintStart_toStartOf="parent" />
    </ConstraintSet>
</MotionScene>